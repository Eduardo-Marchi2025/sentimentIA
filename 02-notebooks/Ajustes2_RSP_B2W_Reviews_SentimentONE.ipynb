      "cell_type": "code",
      "source": [
        "vectorizer = TfidfVectorizer(\n",
        "    max_features=5000,\n",
        "    stop_words=stopwords_pt,\n",
        "    ngram_range=(1, 2),\n",
        "    min_df=2\n",
        ")\n",
        "\n",
        "X = vectorizer.fit_transform(df['texto_processado'])\n",
        "y = df['sentimento']"
